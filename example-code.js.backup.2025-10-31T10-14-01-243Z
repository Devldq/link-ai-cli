// 示例代码文件 - 用于测试代码审查功能
function calculateSum(a, b) {
  return a + b;
}

function calculateProduct(a, b) {
  return a * b;
}

// 这个函数有一些可以改进的地方
function processArray(arr) {
  var result = [];
  for (var i = 0; i < arr.length; i++) {
    if (arr[i] > 0) {
      result.push(arr[i] * 2);
    }
  }
  return result;
}

// 导出函数
module.exports = {
  calculateSum,
  calculateProduct,
  processArray
};